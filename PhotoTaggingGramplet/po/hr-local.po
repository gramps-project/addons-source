msgid "_Select"
msgstr "_Odaberi"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:481
msgid "_Add"
msgstr "_Dodaj"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:484
msgid "_Clear"
msgstr "_Očisti"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:487
msgid "_Remove"
msgstr "_Ukloni"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:634
#, python-format
msgid "Edit Person (%s)"
msgstr "Uredi osobu (%s)"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:690
#, python-brace-format
msgid "Replace to {0}"
msgstr "Zamijeni na {0}"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:791
msgid "Detecting faces..."
msgstr "Pronalaženje lica …"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:816
msgid "Detection finished"
msgstr "Pronalaženje je završeno"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:866
#, python-brace-format
msgid "Another region of this image is associated with {name}. Remove it?"
msgstr ""
"Jedno drugo područje ove slike povezano je s osobom {name}. Želiš li ga "
"ukloniti?"

#: PhotoTaggingGramplet/PhotoTaggingGramplet.py:869
#, python-brace-format
msgid ""
"{count} other regions of this image are associated with {name}. Remove them?"
msgstr ""
"{count} druga područja ove slike povezana su s osobom {name}. Želiš li ih "
"ukloniti?"

